        clear: left;
        float: none;
        margin: 0 0 0 225px;
        width: 550px;
    }
}

@media screen and (max-width:480px)
{
    body { width: 440px; }

    .setup
    {
        float: none;
        width: auto;
    }

    .maintitle .container
    {
        float: none;
        width: auto;
        margin: 0;
    }

    #titleButtons li
    {
      float:none;
      width:auto;
      margin:0;
      dislpay:none;
    }

    .maintitle .container h1
    {
        float: none;
        width: auto;
        margin: 0;
        font-size: 30px;
    }
    .maintitle .container h2
    {
        float: none;
        width: auto;
        margin: 0;
        font-size: 20px;
    }

    .line
    {
        float: none;
        width: auto;
        margin-top: 20px;
    }

    .botmenu
    {
        float:none;
        width:auto;
        margin:0;

    }

    .feature-image { display: none; }
}
