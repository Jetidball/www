.title
{
  margin: 0;
  padding: 0;
  margin-top:2%;
  margin-bottom:2%;
  width:100%;
  z-index: 2;
  position: relative;
  text-align: center;
}
#titleButtons
{
    margin:0;
    padding: 0;
    position: relative;
    width:100%;
    text-align: center;
}
#titleButtons a
{
    margin: 0;
    padding: 0;
    color:#000;
    text-shadow:1px 1px 1px #39B7CD;
}
#titleButtons a:hover
{
  color:#f04b59;
}
#titleButtons ul
{
  width:100%;
  margin:0;
  padding:0;
}
#titleButtons li
{
  display: inline-block;
  padding: 0 10px 6px;
  margin: 0;
  font-family: 14px/17px 'futura', sans-serif;
  text-align:center;
  font-weight:bold;
  letter-spacing: 1px;
  text-transform: uppercase;
  border-bottom: 1px solid blue;
}
.botmenu
{
  bottom: 0;
  width: 22%;
  position: fixed;
}
.botmenu.open-botmenu
{
  bottom: 0;
  width: 22%;
  position: fixed;
  display: block;
}
.botmenu #botmenu-toggle
{
  bottom:0;
  position: relative;
  display: block;
  color: red;
  padding:0;
}
.botmenu .content
{
  position: absolute;
  top: 18%;
  left: 0;
  background: rgba(198, 235, 229, 0.9999);
  right: 0;
  bottom: 0;
  padding: 20px;
  border: 2px solid #ccc;
  overflow: scroll;
  border-top: none;
  white-space: nowrap;

}
.tabs
{
  white-space: nowrap;
  position: relative;
  min-height: 140px;
  clear: both;
  margin: 10px 10px;
  font-size: 16px;
}
.tab
{
  float: left;
}
.tab label
{
  left:0;
  background: #eee;
  padding: 4%;
  padding-right: 4px;
  padding-bottom: 1%;
  padding-top: 5%;
  border: 2px solid grey;
  border-bottom: none;
  margin-left: 0px;
  position:relative;
  left:0px;
  font-size: 16px;
  text-shadow: 1px 1px 1px #ddd;
  white-space: nowrap;
  z-index: 1;
}
.tab [type=radio]
{
  display:none;
}
[type=radio]:checked ~ label
{
  font-weight: bold;
  background: #d4ece8;
  border-bottom: none;
  z-index: 2;
  white-space: nowrap;
}
[type=radio]:checked ~ label ~ .content
{
  z-index: 1;
}
#botmenu-button
{
  display: block;
  text-align: center;
  left:5%;
  bottom:5%;
  z-index: 5;
  font-weight: bold;
  background-color: #000;
  position: fixed;
  color: red;
  padding:0.2%;
}
#botmenu-toggle
{
  display: block;
  text-align: center;
  left:20%;
  bottom:13%;
  z-index: 5;
  font-weight: bold;
  background-color: #000;
  position: fixed;
  color: red;
  padding:0.2%;
}
#marquee a
{
  border: 10px solid #04152f;
  display: inline-block;
  text-align: center;
  text-decoration: underline;
  background-color: #000;
  background-image: url("epiclootlogoBOXColor.png");
  background-size: contain;
  background-repeat: no-repeat;
  background-position: bottom;
  border-image: url("epiclootlogoBOXColor.png");
  border-image-width: 13px;
  color: #fff;
  font-size: 25px;
  margin-top: 5%;
  padding:10px;
}
#marquee a:hover
{
  display: inline-block;
  background-color: #3065b5;
  background-position: top;
  color: #0ee616;
  font-size: 25px;
}
@media screen and (max-width:999px)
{
    body { width: 800px; }

    #extras
